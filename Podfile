# Uncomment the next line to define a global platform for your project
platform :ios, '11.0'

target 'SSO' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for SSO
  pod 'FacebookLogin'
  pod 'FacebookCore'
  pod 'GoogleSignIn'
  pod 'TwitterKit'
  pod 'Kingfisher'
  pod 'NVActivityIndicatorView'
  pod 'Material'

end
